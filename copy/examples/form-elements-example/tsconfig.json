{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "paths": {
      "components": [
        "projects/components/src/public_api"
      ],
      "components/*": [
        "projects/components/*"
      ],
      "@uxdf/ioc-model": [
        "../../main/projects/ioc-model/src/public_api"
      ],
      "@uxdf/ioc-model/*": [
        "../../main/projects/ioc-model/model/*"
      ],
      "@uxdf/ioc-engine": [
        "../../main/projects/ioc-engine/src/public_api"
      ],
      "@uxdf/ioc-engine/*": [
        "../../main/projects/ioc-engine/*"
      ],
      "@uxdf/ioc-engine-rx": [
        "../../main/projects/ioc-engine-rx/src/public_api"
      ],
      "@uxdf/ioc-engine-rx/*": [
        "../../main/projects/ioc-engine-rx/*"
      ],
      "design-factory-v2": [
        "../../libraries/design-factory-v2/projects/design-factory-v2/src/public_api"
      ],
      "design-factory-v2/*": [
        "../../libraries/design-factory-v2/projects/design-factory-v2/*"
      ]
    }
  }
}